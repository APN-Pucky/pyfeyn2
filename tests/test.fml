<?xml version="1.0"?>
<feynml>
 <head>
   <meta name="creator" value="PyFeyn"/>
   <description>
   A sample diagram showing a QCD correction to the Drell-Yan process.
   </description>
 </head>
 <diagram legs="4">
   <label  text="Drell-Yan QCD vertex correction" x="0" y="2">
      <id>L0</id>
   </label>
   <vertex id="V0" x="-2.0" y="0.0">
      <id>V0</id>
      </vertex>
   <vertex id="V1" x="2.0" y="0.0">
      <id>V1</id>
   </vertex>
   <leg id="E0" type="fermion" sense="incoming" target="V0" x="-4.0" y="2.0"
      label="e-" style="arrow-pos:0.53;">
      <id>E0</id>
      <sense>incoming</sense>
      <target>V0</target>
      </leg>
   <leg id="E1" type="fermion" sense="outgoing" target="V0" x="-4.0" y="-2.0"
      label="e+" style="arrow-pos:0.53; parallel-arrow-displace:-0.06; parallel-arrow-sense:-1;">
      <id>E1</id>
      <sense>outgoing</sense>
      <target>V0</target>
      </leg>
   <propagator id="P2" type="photon" source="V0" target="V1" label="Z">
      <id>P2</id>
      <source>V0</source>
      <target>V1</target>
   </propagator>
   <vertex id="V5" x="3.0" y="-1.0">
   <id>V5</id>
   </vertex>
   <propagator id="P4" type="fermion" source="V5" target="V1">
   <id>P4</id>
   <source>V5</source>
   <target>V1</target>
   </propagator>
   <vertex id="V6" x="3.0" y="1.0">
   <id>V6</id>
   </vertex>
   <propagator id="P5" type="fermion" source="V1" target="V6">
   <id>P5</id>
   <source>V1</source>
   <target>V6</target>
   </propagator>
   <propagator id="P7" type="gluon" source="V5" target="V6" bend="0.5"
      label="g" style="parallel-arrow-displace:0.2;">
      <id>P7</id>
      <source>V5</source>  
      <target>V6</target>
   </propagator>
   <leg id="E2" type="fermion" sense="incoming" target="V5" x="4.0" y="-2.0"
      label="q" style="arrow-pos:0.53; parallel-arrow-displace:-0.06; parallel-arrow-sense:-1;">
      <id>E2</id>
      <sense>incoming</sense>
      <target>V5</target>
      </leg>
   <leg id="E3" type="fermion" sense="outgoing" target="V6" x="4.0" y="2.0"
      label="q" style="arrow-pos:0.53;">
      <id>E3</id>
      <sense>outgoing</sense>
      <target>V6</target>
      </leg>
 </diagram>
</feynml>
